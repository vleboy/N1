{
  "name": "api-visual",
  "version": "1.0.0",
  "scripts": {
    "start": "node app.js",
    "logs": "docker logs api-visual_api-visual_1 -f",
    "compose-up": "docker-compose -f docker-compose.yml up -d --build",
    "compose-down": "docker-compose -f docker-compose.yml down",
    "compose-restart": "docker-compose -f docker-compose.yml down && docker-compose -f docker-compose.yml up -d --build",
    "stack-deploy": "docker stack deploy --prune -c docker-compose.yml v1",
    "stack-rm": "docker stack rm v1"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "aws-sdk": "^2.437.0",
    "axios": "^0.18.0",
    "config": "^3.1.0",
    "dayjs": "^1.8.12",
    "ip2region": "^1.6.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-xauth": "^1.2.0",
    "koa-xerror": "^1.1.0",
    "koa-xlog": "^1.1.0",
    "lodash": "^4.17.11",
    "node-cron": "^2.0.3",
    "nodebatis": "^2.3.7",
    "number-precision": "^1.3.1",
    "tracer": "^0.9.8"
  }
}